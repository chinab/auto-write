<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<typeHandler
		callback="org.springframework.orm.ibatis.support.ClobStringTypeHandler"
		jdbcType="CLOB" javaType="java.lang.String" />


	<sqlMap resource="sqlmap/User_SQL.xml" />
	<sqlMap resource="sqlmap/Common_SQL.xml" />
	<sqlMap resource="sqlmap/Admin_SQL.xml" />
	<sqlMap resource="sqlmap/Board_SQL.xml" />
	<sqlMap resource="sqlmap/Memo_SQL.xml" />
	<sqlMap resource="sqlmap/AutoWrite_SQL.xml" />
</sqlMapConfig>
